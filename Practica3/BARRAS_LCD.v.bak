module BARRAS_LCD(
	input CLK, RST_n,
	output nCLK, GREST, HD, VD, DEN,
	output [7:0] R, G, B
	)
	
	wire [9:0 ]Filas;
	wire [10:0] Columnas;
	wire [7:0] Roux, Goux, Boux;
	
	LCD_SYNC control_LCD (.CLK(CLK),
								.RST_n(RST_n),
								.NCLK(NCLK),
								.GREST(GREST),
								.HD(HD),
								.VD(VD),
								.DEN(DEN),
								.Columna(Columnas),
								.Fila(Filas));
	
	
	
	
	
endmodule